<div class="questPage">
    <h1>Quests</h1>
    <a href="/quests/new">New Quest</a>

    <% quests.forEach(quest => { %> 
        <div class="questContainer">
            <p><%= quest.title %></p> 
        </div>

        <a href="quests/<%= quest.slug %> ">show</a>

        <form action="/quests/<%= quest.id %>?_method=DELETE" method="POST">
            <button type="submit">delete</button>
        </form>
    <% }); %> 
    
</div>