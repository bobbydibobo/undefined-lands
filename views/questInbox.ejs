<h1>QUESTS</h1>
<div class="blogArea">
    <% quests.forEach(quest => { %>
      <div class="postArea">
          <h4 class="postTitle"><%= quest.title %>!</h4>

          <div class="actionBtns">
            <a href="/game/quests/:id" class="readMoreBtn"><svg xmlns="http://www.w3.org/2000/svg" width="50" height="50" fill="rgb(245, 245, 245)" viewBox="0 0 24 24"><path d="M16 8v-4l8 8-8 8v-4h-5v-8h5zm-7 0h-2v8h2v-8zm-4.014 0h-1.986v8h1.986v-8zm-3.986 0h-1v8h1v-8z"/></svg></a>
          </div>
      </div>
    <% }) %>
</div>